{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@3.1.30",
            "config": {
                "runtime": "python3.8",
                "maxLambdaSize": "15mb",
                "installCommand": "pip install -r requirements.txt"
            }
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}
